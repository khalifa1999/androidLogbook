<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.example.topquiz">

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dpm"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".suppaddInfo" />
        <activity android:name=".suppdaily" />
        <activity android:name=".supparametre" />
        <activity android:name=".affichage" />
        <activity android:name=".parametre" />
        <activity android:name=".choose" />
        <activity android:name=".daily" />
        <activity android:name=".addInfo" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    //donner la permission a mon application d'avoir accès à internet
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>